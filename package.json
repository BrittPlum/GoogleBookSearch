{
  "name": "googlebooksearch",
  "version": "1.0.0",
  "description": "React Book Search",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'googlebooksearch/*'\" \"npm run googlebooksearch\"",
    "googlebooksearch": "googlebooksearch",
    "install": "cd googlebooksearch && npm install",
    "build": "cd googlebooksearch && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^4.1.0",
    "nodemon": "^1.18.7"
  },
  "dependencies": {
    "express": "^4.16.3",
    "if-env": "^1.0.4"
  }
}
